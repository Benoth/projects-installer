# Description: {{ item.name }}

PROJECT_PATH="{{ item.path | default(path) }}/{{ item.repo | basename | regex_replace('.git$', '') }}"
PROJECT_DB="{{ item.repo | basename | regex_replace('.git$', '') }}"
PROJECT_SQL="${PROJECT_PATH}/sql/${PROJECT_DB}.sql"

source ~/.desk/base-project.sh $PROJECT_PATH
source ~/.desk/web-project.sh $PROJECT_PATH $PROJECT_DB $PROJECT_SQL
